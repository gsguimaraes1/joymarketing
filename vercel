        ];

        let currentIndex = 0;

        function switchPage() {
            document.getElementById('powerbi-frame').src = urls[currentIndex];
            currentIndex = (currentIndex + 1) % urls.length;
        }

        setInterval(switchPage, 60000); // Alterna a cada 6 segundos (6000 ms)
    </script>
</head>
<body onload="switchPage()">
    <iframe id="powerbi-frame"></iframe>
